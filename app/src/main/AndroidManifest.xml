<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mar.mardip2">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <!--<uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".MedRecord.AddMedRecordActivity"
            android:label="@string/title_add_med_record" />
        <activity android:name=".MedRecord.MedRecordActivity"
            android:label="@string/title_med_record"  />
        <activity
            android:name=".HealthIndicators.HealthIndicatorsActivity"
            android:label="@string/Health_indicators" />
        <activity android:name=".HealthIndicators.History.HealthIndicatorHist" />
        <activity
            android:name=".CalendarActivity"
            android:label="@string/title_activity_calendar" />
        <activity
            android:name=".Symptoms"
            android:label="@string/Symptoms" />
        <activity
            android:name=".AnalActivity"
            android:label="@string/title_activity_anal"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>


</manifest>